#include <iostream>
using namespace std;

// Function to add two numbers
int addNumbers(int a, int b) {
    return a + b;
}

// Function to multiply two numbers
int multiplyNumbers(int a, int b) {
    return a * b;
}

int main() {
    int num1 = 5;
    int num2 = 10;

    // Error 1: Misspelled function name (should be addNumbers)
